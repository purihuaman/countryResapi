template#template
	a(href="" data-link='') 
		article.flag(data-flag="flag")
			.flag__container-img
				img(
					src="./ad.svg"
					alt=""
					data-picture="picture"
					class="flag__image"
				)

			.flag__body(data-body='flag-body')
				h3.flag__title(data-country="country") Germany

				.flag__details
					p.flag__text(data-nativename='native')

					p.flag__text
						b.flag__subtitle Population: 
						span(data-population="population") 81,770,900

					p.flag__text
						b.flag__subtitle Region: 
						span(data-region="region") Europe
					
					p.flag__text(data-subregion='subregion')

					p.flag__text
						b.flag__subtitle Capital: 
						span(data-capital="capital") Berlin

					.flag__others
						p.flag__text(data-tld='tld')

		